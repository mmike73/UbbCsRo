<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.socialnetworkjavafx.UIs.Controllers.AllUsers"
            prefHeight="400.0" prefWidth="600.0">

    <VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          prefWidth="400.0" prefHeight="300.0">
        <Label text="All Users" style="-fx-font-size: 16px; -fx-font-weight: bold;"/>
        <TableView fx:id="usersTable" prefWidth="380.0" prefHeight="250.0">
            <columns>
                <TableColumn fx:id="usernameColumn" text="Username" />
                <TableColumn fx:id="passwordColumn" text="Password" />
            </columns>
        </TableView>
    </VBox>

</AnchorPane>
